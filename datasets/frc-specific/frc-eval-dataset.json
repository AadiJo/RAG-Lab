{
  "id": "frc-eval-dataset",
  "name": "FRC RAG Evaluation Dataset",
  "description": "Comprehensive test cases for evaluating FRC-specific RAG retrieval and generation quality. Covers mechanisms, drivetrains, sensors, autonomous, and multi-modal queries.",
  "version": "1.0.0",
  "createdAt": "2024-12-13T00:00:00.000Z",
