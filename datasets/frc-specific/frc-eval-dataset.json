  "testCases": [
    {
      "id": "intake-note-2024",
      "query": "Design a versatile intake mechanism for the 2024 CRESCENDO season that can handle Note game pieces. The intake must be able to pick up Notes from the ground, accept handoffs from human players at the Source, and work reliably in autonomous mode.",
      "category": "mechanisms",
      "difficulty": "hard",
      "tags": ["intake", "2024", "crescendo", "note"],
      "groundTruth": {
        "expectedKeywords": ["intake", "note", "roller", "belt", "motor", "neo", "ground", "source", "autonomous", "sensor", "beam break", "foam", "wheel", "polycarbonate", "compliant"],
        "relevantChunks": ["intake mechanism design", "ground pickup system", "note handling"],
        "expectedImages": ["intake", "roller", "ground pickup"]
      }
    },
    {
      "id": "drivetrain-speed-calc",
